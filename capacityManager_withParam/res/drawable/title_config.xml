<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    
    <!-- 按钮按下时显示bg_pressed图片 -->
    <item 
        android:state_pressed="true"
        android:drawable="@drawable/menu_btn_press"/>
    <item android:drawable="@drawable/menu_btn_press" android:state_selected="true"/>
    
    <!-- 按钮抬起的时候显示bg_normal图片 -->
    <item 
        android:state_pressed="false"
        android:drawable="@drawable/menu_btn_normal"/>

    

</selector>
